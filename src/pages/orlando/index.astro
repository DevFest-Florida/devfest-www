---
import Layout from '~/pages/orlando/PageLayout.astro';
import EventDetails from './EventDetails.astro';
import Location from './Location.astro';
import Sponsors from './Sponsors.astro';

const metadata = {
  title:
    'DevFest Florida is group of GDG chapters from around the state of Florida. We work together to bring technologists and developers from the Florida area to code, learn, have fun, and give back.',
  ignoreTitleTemplate: true,
};
---
<Layout metadata={metadata}>
  <div>
    <EventDetails></EventDetails>
    <Location></Location>
    <Sponsors></Sponsors>
  </div>
</Layout>
