---
import favIcon from '~/assets/favicons/favicon.ico';
import appleTouchIcon from '~/assets/favicons/logo192.png';
import favIconPng from '~/assets/favicons/logo512.png';
---

<link rel="shortcut icon" href={favIcon} />
<link rel="icon" type="image/png" href={favIconPng.src} />
<link rel="mask-icon" href={favIconPng.src} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
